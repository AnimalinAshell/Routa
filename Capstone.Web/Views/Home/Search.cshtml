@{
    ViewBag.Title = "Search";
}

<newsearch></newsearch>
<newmap></newmap>
<newlist></newlist>



<!-- Reference the riot tags here -->
<script src="~/Content/newsearch.tag" type="riot/tag">
</script>
<script src="~/Content/newmap.tag" type="riot/tag">
</script>
<script src="~/Content/newlist.tag" type="riot/tag">
</script>

<script>
    let bus = {};

    riot.observable(bus); //makes it so our bus can send and receive messages
    // Mount the riot tags to the page here
    riot.mount('newsearch', { bus: bus });
    riot.mount('newmap', { bus: bus });
    riot.mount('newlist', { bus: bus });

</script>
